<div class="bookings_box">

<h1>Bokningsförfrågningar</h1>

<table>

  <thead>
  <tr class="b_head">
    <th class="b_head2">Klubb</th>
    <th class="b_head2">Film</th>
  </tr>
  </thead>

  <tbody id="pending_bookings">
  <%= render partial: 'pending_bookings' %>
  </tbody>
</table>

<h1 class="bookings_h1">Godkända Bokningar</h1>
  <table>

    <thead>
    <tr class="b_head">
      <th class="b_head2">Klubb</th>
      <th class="b_head2">Film</th>
    </tr>
    </thead>

    <tbody id="approved_bookings">
    <%= render partial: 'approved_bookings' %>
    </tbody>
  </table>

  <h1 class="bookings_h1">Arkiverade/klara Bokningar</h1>
  <table>

    <thead>
    <tr class="b_head">
      <th class="b_head2">Klubb</th>
      <th class="b_head2">Film</th>
    </tr>
    </thead>

    <tbody id="bookings_done">
    <%= render partial: 'bookings_done' %>
