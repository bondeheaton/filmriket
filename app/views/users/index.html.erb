        <th data-hide="phone">Telefonnummer</th>
        <th data-hide="phone">Poäng</th>
        <th data-hide="phone" colspan="4"></th>
      </tr>
    </thead>

    <tbody id="admin_approved">
    <%= render 'users/admin_approve'%>
    </tbody>
  </table>
  </div>
  <br>


  <hr>
