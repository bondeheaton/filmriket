<head xmlns="http://www.w3.org/1999/html">
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
</head>


<div class="row">

<div class="users_box">

  <div class="small-12 small-centered columns">
  <h1 class="users_h1">Väntar på godkännade</h1>

    <table class="footable table">
      <thead>
      <tr>
        <th data-hide="phone">Förnamn</th>
        <th data-hide="phone">Efternamn</th>
        <th data-toggle="true">Email</th>
        <th data-hide="phone">Födelsedatum</th>
        <th data-hide="phone">Telefonnummer</th>
        <th data-hide="phone">Poäng</th>
        <th data-hide="phone" colspan="4"></th>
      </tr>
    </thead>

    <tbody id="not_approved">
    <%= render 'users/not_approved' %>
    </tbody>
  </table>
  </div>


  <div class="small-12 small-centered columns">
  <h1 class="users_h1">Godkända Användare</h1>

  <table class="footable table">
    <thead>
    <tr>
      <th data-hide="phone">Förnamn</th>
      <th data-hide="phone">Efternamn</th>
      <th data-toggle="true">Email</th>
      <th data-hide="phone">Födelsedatum</th>
      <th data-hide="phone">Telefonnummer</th>
      <th data-hide="phone">Poäng</th>
      <th data-hide="phone" colspan="4"></th>
    </tr>
    </thead>


    <tbody id="approved">
    <%= render 'users/approve' %>
    </tbody>
  </table>
  </div>

  <div class="small-11 small-centered columns">
  <h1 class="users_h1">Admin-Användare</h1>


    <table class="footable table">
      <thead>
