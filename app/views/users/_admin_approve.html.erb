<% @admin_users.each do |user| %>
    <tr>
      <td><%= user.firstname %></td>
      <td><%= user.lastname %></td>
      <td><%= user.email %></td>
      <td style="width: 10px;"><%= user.access %></td>
      <td>0<%= user.phonenumber %></td>
      <td><%= user.points %></td>
      <td><%= link_to 'Ändra', edit_user_path(user), :class => "button tiny radius right_button_edit" %></td>
      <td><%= link_to 'Förstör', user, method: :delete, data: { confirm: 'Är du säker?' }, :class => "button tiny radius alert right_button_destroy" %></td>
      <td><%= link_to "Neka", disapprove_user_path(user.id), :class => "button tiny radius alert access right_button_deny" %></td>
    </tr>
<% end %>