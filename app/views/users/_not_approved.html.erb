<% @not_approved_users.each do |user| %>
    <tr>
      <td><%= user.firstname %></td>
      <td><%= user.lastname %></td>
      <td><%= user.email %></td>
      <td><%= user.personalnumber %></td>
      <td>0<%= user.phonenumber %></td>
      <td><%= user.points %></td>
      <td><%= link_to 'Ändra', edit_user_path(user), :class => "button tiny radius right_button_edit2" %></td>
      <td><%= link_to 'Förstör', user, method: :delete, data: { confirm: 'Är du säker?' }, :class => "button tiny radius alert right_button_destroy2" %></td>
      <td><%= link_to "Godkänn", approve_user_path(user.id), :class => "button tiny radius success access right_button_approve2" %></td>
    </tr>
<% end %>